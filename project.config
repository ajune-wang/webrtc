[access "refs/*"]
	owner = group webrtc-admins
[access "refs/gnumbd-config/main"]
	create = group webrtc-admins
	push = group webrtc-admins
[access "refs/gsubtreed-config/main"]
	create = group webrtc-admins
	push = group webrtc-admins
[access "refs/branch-heads/*"]
	create = group git-numbering-bot
	forgeAuthor = group git-numbering-bot
	forgeCommitter = group git-numbering-bot
	push = group git-numbering-bot
[access "refs/heads/*"]
	create = group git-numbering-bot
	forgeAuthor = group git-numbering-bot
	forgeCommitter = group git-numbering-bot
	push = group git-numbering-bot
[access "refs/pending-tags/*"]
	create = group git-numbering-bot
	forgeAuthor = group git-numbering-bot
	forgeCommitter = group git-numbering-bot
	push = group git-numbering-bot
[access "refs/pending/*"]
	push = group webrtc-committers
[access "refs/pending/branch-heads/*"]
	push = group webrtc-committers
[access "refs/old-branches/*"]
	create = group webrtc-admins
	push = group webrtc-admins
	forgeAuthor = group webrtc-admins
	forgeCommitter = group webrtc-admins
