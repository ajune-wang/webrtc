[access]
	inheritFrom = All-Projects
[submit]
	action = rebase always

[label "Commit-Queue"]
	value = 0 Not ready
	value = +1 Dry run
	value = +2 Commit
	defaultValue = 0
	function = NoBlock
	copyAllScoresIfNoChange = false

[access "refs/for/refs/*"]
	addPatchSet = block group chrome-commit-bot
[access "refs/heads/*"]
	push = block group chrome-commit-bot
	forgeAuthor = block group chrome-commit-bot
	label-Commit-Queue = block group Logged-in Public Users
	labelAs-Commit-Queue = block group chrome-commit-bot
	submit = block group chrome-commit-bot

[access "refs/branch-heads/*"]
	create = block group webrtc-releasers
	forgeAuthor = block group chrome-commit-bot
	forgeAuthor = block group webrtc-releasers
	forgeCommitter = block group webrtc-releasers
	push = block group chrome-commit-bot
	push = block group cria/project-webrtc-committers
	push = block +force group webrtc-releasers
	abandon = block group cria/project-webrtc-committers
	label-Code-Review = block group cria/project-webrtc-committers
	submit = block group cria/project-webrtc-committers

[access "refs/old-branches/*"]
	create = block group cria/project-webrtc-admins
	label-Code-Review = block group cria/project-webrtc-admins
	submit = block group cria/project-webrtc-admins
	abandon = block group cria/project-webrtc-admins
	push = block group cria/project-webrtc-admins
	forgeAuthor = block group cria/project-webrtc-admins
	forgeCommitter = block group cria/project-webrtc-admins
[access "refs/heads/infra/config"]
	label-Code-Review = block group cria/project-webrtc-admins
	submit = block group cria/project-webrtc-admins
	abandon = block group cria/project-webrtc-admins
	push = block group cria/project-webrtc-admins
	forgeAuthor = block group cria/project-webrtc-admins
[access "refs/gsubtreed-config/main"]
	label-Code-Review = block group cria/project-webrtc-admins
	submit = block group cria/project-webrtc-admins
	abandon = block group cria/project-webrtc-admins
	push = block group cria/project-webrtc-admins
	forgeAuthor = block group cria/project-webrtc-admins

[access "refs/heads/lkgr"]
	push = block group lkgr-updaters
[access "refs/*"]
	owner = block group cria/project-webrtc-admins
[plugin "git-numberer"]
	generate-enabled-refglob = refs/heads/master
	generate-enabled-refglob = refs/branch-heads/*
	validate-enabled-refglob = refs/heads/master
	validate-enabled-refglob = refs/branch-heads/*
